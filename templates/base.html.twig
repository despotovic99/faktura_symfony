<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {#            <!-- Copy CSS from https://getbootstrap.com/docs/5.0/getting-started/introduction/#css --> #}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
              integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
              crossorigin="anonymous">
        <link href="{{ asset('assets/css/base.css') }}" rel="stylesheet">
    {% endblock %}
    {% block cssStrane %}

    {% endblock %}
    {% block javascripts %}
        {#            <!-- Copy JavaScript from https://getbootstrap.com/docs/5.0/getting-started/introduction/#js --> #}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
                crossorigin="anonymous"></script>
    {% endblock %}
</head>
<body>
{% block body %}

    {% block pageContainer %}
        <div class="page-container">
            {% block headerContainer %}
                <div class="header-container">
                    {% include('includes/_navbar.html.twig') %}
                </div>
            {% endblock %}

            {% block bodyContainer %}{% endblock %}

        </div>
    {% endblock %}

{% endblock %}
{% block customjavascripts %}{% endblock %}
</body>
</html>
